<?xml version="1.0" encoding="utf-8" ?>
<widget id="GroupBoxPlus.widget.GroupBoxPlusNoContext" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>GroupBoxPlus (No Context)</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
        <property key="mfStartOpen" type="microflow" required="false">
            <caption>Start Open microflow</caption>
            <category>Appearance</category>
            <description>This microflow should return `true` or `false`. If true, the groupbox will start open, else closed.</description>
            <returnType type="Boolean"></returnType>
        </property>
        <property key="roleMaps" type="object" required="false" isList="true">
            <caption>Appearance per Role</caption>
            <category>Appearance</category>
            <description>The message visible within the widget.</description>
            <properties>
                <property key="role" type="string">
                    <caption>Role</caption>
                    <category>Appearance</category>
                    <description>The user role.</description>
                </property>
                <property key="view" type="boolean" defaultValue="true">
                    <caption>Start Open?</caption>
                    <category>Appearance</category>
                    <description>If yes, the group box starts expanded, else it starts collapsed.</description>
                </property>
            </properties>
        </property>
    </properties>
</widget>
